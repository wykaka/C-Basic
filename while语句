//求1-100中3的倍数
int main()
{
	int a = 1;
	while (a <= 100)
	{
		if (0 == a % 3)
			printf("%d\n", a);
		a++;
	}
	return 0;
}

//课程练习
int main()
{
	int line=0;
	printf("加入码农\n");
	while (line < 100)
	{
		printf("敲一行代码：%d\n",line);
		line++;
	}
	if(line>=100)
	    printf("好offer\n");
	return 0;
}

//输出1-100中所有的奇数
int main()
{
	int num1=1;	
 	int num2=0;
	while(num1<=100)
	{
		num2 = num1 % 2;
		if(num2==1)
			printf("%d ", num1);		
		num1++;	
	}
	return 0;
}

